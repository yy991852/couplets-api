{
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": ".", 
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}