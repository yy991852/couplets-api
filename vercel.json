{
  "version": 2,
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/$1.js"
    }
  ],
  "includeFiles": [
    "models/**/*",
    "database/**/*"
  ]
}